{
  "name": "react-lib",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "react-lib/src",
  "projectType": "library",
  "tags": [],
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/react-lib",
        "main": "react-lib/src/index.ts",
        "tsConfig": "react-lib/tsconfig.lib.json"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}